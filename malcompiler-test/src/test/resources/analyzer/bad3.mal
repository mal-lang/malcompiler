#id: "badvar"
#version: "1.0.0"
category System {
  asset Alpha {
  	let VAR = charlies.alphas
    | compromise
      -> VAR().deltas.write
  }
  asset SubAlpha extends Alpha {
  	let VAR = charlies.alphas
  	| access
  }
  asset Bravo {
    let VAR1 = alphas
    | access
      -> VAR1.compromise
  }
  asset Charlie {
    | read
  }
  asset Delta {
    | write
  }
}
associations {
  Alpha [alphas] * <-- _ --> * [bravos] Bravo
  Alpha [alphas] * <-- _ --> * [deltas] Delta
  Alpha [alphas] * <-- _ --> * [charlies] Charlie
  Charlie [charlies] * <-- _ --> * [bravos] Bravo
}
