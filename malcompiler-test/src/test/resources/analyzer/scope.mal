#id: "id"
#version: "0.0.0"
category cat {
  asset AA {
    // define variable 'var' visible by '& access' and '| compromise'
    let var = c
    & access ->
      // redefine 'var'
      let var = b,
      var.compromise
    | compromise ->
      // variable 'var' should be as defined on L4
      var.read
  }
  asset B {
    | compromise
  }
  asset CC {
    | read
  }
}
associations {
  AA [a] * <-- ab --> * [b] B
  AA [a] * <-- ac --> * [c] CC
}
