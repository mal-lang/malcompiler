category System
{
  abstract asset SomeLongNameThatIsLong extends
    SomeOtherName
  {
    let variable1 =
      steps.that.exists
        /\
        combined.with.some.other.steps
    let variable2 = even.more.steps[Step]

    | compromise
      -> variable1()[SomeOtherName].substeps*
           .local.read,
         (variable2().after.open
            -
            well.named.steps.written)
  }
}
